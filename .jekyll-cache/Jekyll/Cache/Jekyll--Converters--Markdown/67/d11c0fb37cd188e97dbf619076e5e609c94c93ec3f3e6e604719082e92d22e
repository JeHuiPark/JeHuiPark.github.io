  <span class="nc">Map</span><span class="o">&lt;</span><span class="nc">String</span><span class="o">,</span> <span class="nc">Object</span><span class="o">&gt;</span> <span class="n">map</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">HashMap</span><span class="o">&lt;</span><span class="nc">String</span><span class="o">,</span> <span class="nc">Object</span><span class="o">&gt;();</span>
  <span class="n">map</span><span class="o">.</span><span class="na">put</span><span class="o">(</span><span class="s">"intParam"</span><span class="o">,</span> <span class="mi">1</span><span class="o">);</span>
  <span class="n">map</span><span class="o">.</span><span class="na">put</span><span class="o">(</span><span class="s">"stringParam"</span><span class="o">,</span> <span class="s">"2"</span><span class="o">);</span>
  <span class="n">mapper</span><span class="o">.</span><span class="na">parseTest1</span><span class="o">(</span><span class="n">map</span><span class="o">);</span>
  <span class="n">mapper</span><span class="o">.</span><span class="na">parseTest2</span><span class="o">(</span><span class="n">map</span><span class="o">);</span>
<span class="o">}</span>
</code></pre></div></div>

